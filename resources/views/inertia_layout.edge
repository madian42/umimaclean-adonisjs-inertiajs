<!DOCTYPE html>
<html>

  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <link
      rel="icon"
      type="image/png"
      sizes="32x32"
      href="{{ asset('resources/images/favicon-32x32.png') }}"
    />
    <link
      rel="icon"
      type="image/png"
      sizes="16x16"
      href="{{ asset('resources/images/favicon-16x16.png') }}"
    />

    <!-- SEO -->
    <meta
      name="description"
      content="UmimaClean adalah jasa cuci sepatu di Bandung dengan layanan antar-jemput. Bersih, cepat, dan aman untuk sneakers dan sepatu kesayangan Anda."
    />
    <meta
      name="keywords"
      content="UmimaClean, cuci sepatu Bandung, laundry sepatu, antar jemput sepatu, perawatan sepatu, cuci sneakers"
    />
    <meta name="author" content="UmimaClean" />

    <!-- Open Graph / Facebook -->
    <meta property="og:type" content="website" />
    <!-- NOTE: Update og:url to your production domain when available -->
    <meta property="og:url" content="https://github.com/filipebraida/adonisjs-starter-kit" />
    <meta property="og:title" content="UmimaClean — Cuci Sepatu Bandung Antar Jemput" />
    <meta
      property="og:description"
      content="Jasa cuci sepatu Bandung dengan layanan antar-jemput. Perawatan profesional untuk sneakers dan sepatu favorit Anda."
    />
    <meta
      property="og:image"
      content="https://raw.githubusercontent.com/filipebraida/adonisjs-starter-kit/main/.github/screenshot.png"
    />

    <!-- Twitter -->
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:title" content="UmimaClean — Cuci Sepatu Bandung Antar Jemput" />
    <meta
      name="twitter:description"
      content="Jasa cuci sepatu Bandung dengan layanan antar-jemput yang praktis dan terpercaya."
    />
    <meta
      name="twitter:image"
      content="https://raw.githubusercontent.com/filipebraida/adonisjs-starter-kit/main/.github/screenshot.png"
    />

    <title inertia>
      UmimaClean — Cuci Sepatu Bandung Antar Jemput
    </title>

    @stack('dumper')
    @viteReactRefresh()
    @inertiaHead()
    @vite(['inertia/app/app.tsx', `inertia/pages/${page.component}.tsx`])
  </head>

  <body class="font-poppins antialiased bg-gray-400">
    @inertia()
  </body>

</html>
